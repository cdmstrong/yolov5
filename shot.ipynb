    "import cv2\n",
    "img = cv2.imread('./shot.jpg')\n",
    "print(img.shape)\n",
    "img = cv2.resize(img, (int(img.shape[0]/3), int(img.shape[1]/3)))\n",
    "img_gray = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)\n",
    "_, out = cv2.threshold(img_gray, 128, 255, 0)\n",
    "cv2.imshow('h', out)\n",
    "cv2.waitKey(0)\n",
    "\n"
   ]
